@{
    ViewData["Title"] = "Home";
    Layout = "_Layout_Tutor";
}

<div class="row justify-content-md-start">
    <div class="col-md-4">
        <div class="card">
            <div class="card-header">
                Welcome
            </div>
            <div class="card-body">
                @if (!string.IsNullOrEmpty(ViewBag.UserEmail))
                {
                    <p>Email: @ViewBag.UserEmail</p>
                }
            </div>
        </div>

    </div>
</div>
